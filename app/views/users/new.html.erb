<h1>Signup</h1>
<p>
	<%= form_for :user, url: '/users' do |f| %>
		<table>
			<tr><td>Name:</td><td><%= f.text_field :name %></td></tr>
			<tr><td>Email:</td><td><%= f.text_field :email %></td></tr>
			<tr><td>Password:</td><td><%= f.password_field :password %></td></tr>
			<tr><td>Password Confirmation:</td><td><%= f.password_field :password_confirmation %></td></tr>
		</table>
		<br>
		<%= f.submit "Submit" %>
	<% end %>
</p>

